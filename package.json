{
  "name": "be-creamcats",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "test": "jest --coverage --verbose --passWithNoTests --detectOpenHandles --forceExit",
    "db-seed": "npx prisma db seed --preview-feature",
    "db-migrate": "npx prisma migrate dev",
    "db-generate": "npx prisma generate",
    "db-push": "npx prisma db push",
    "db-reset": "npx prisma migrate reset",
    "swagger-edit": "npx opener http://localhost:$(grep SWAGGER_EDITOR_HOST_PORT .env | cut -d '=' -f2)/",
    "api-doc": "npx opener http://localhost:$(grep APP_HOST_PORT .env | cut -d '=' -f2)/api-docs"
  },
